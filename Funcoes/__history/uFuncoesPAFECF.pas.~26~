    Q.SQLConnection := ConexaoDados;
    Q.Close;
    Q.SQL.Clear;
    Q.SQL.Add('update bico_comunica bi set');
    Q.SQL.Add('bi.impresso = '+IntToStr(pImpresso));
    Q.SQL.Add('where bi.id = :pID');
    Q.Params.ParamByName('pID').AsInteger := pID;
    Q.ExecSQL;
  finally
    FreeAndNil(Q);
  end;

end;

