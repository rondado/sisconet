  ImprimeRelatorioGerencial('                                            ');
  ImprimeRelatorioGerencial('                                            ');
  ImprimeRelatorioGerencial('   --------------------------------------   ');
  ImprimeRelatorioGerencial('           ASSINATURA DO CLIENTE            ');
  ImprimeRelatorioGerencial('                                            ');
  ImprimeRelatorioGerencial('     RECONHEÇO E PAGAREI A DIVIDA ACIMA     ');
